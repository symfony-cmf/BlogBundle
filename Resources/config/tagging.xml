<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="symfony_cmf_blog.tagging_strategy_phpcr" class="Symfony\Cmf\Bundle\BlogBundle\Tagging\PHPCRStrategy">
            <argument type="service" id="symfony_cmf_blog.blog_repository"/>
            <argument type="service" id="symfony_cmf_blog.post_repository"/>
        </service>

    </services>

</container>



